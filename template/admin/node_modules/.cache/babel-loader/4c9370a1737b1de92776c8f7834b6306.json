n\n$scroll.remove = function (fn) {\n  fn._index && events.splice(fn._index, 1);\n};\n\n//上拉加载；\nconst Scroll = {\n  addHandler: function (element, type, handler) {\n    if (element.addEventListener) element.addEventListener(type, handler, false);\n    else if (element.attachEvent) element.attachEvent('on' + type, handler);\n    else element['on' + type] = handler;\n  },\n  listenTouchDirection: function () {\n    this.addHandler(window, 'scroll', function () {\n      const wh = window.innerHeight,\n        st = window.scrollY;\n      events\n        .filter((e) => e.dom.scrollHeight && e.dom.scrollHeight > 0)\n        .forEach((e) => {\n          var dh = e.dom.scrollHeight;\n          var s = Math.ceil((st / (dh - wh)) * 100);\n          if (s > 85) e.fn();\n        });\n    });\n  },\n};\n\nScroll.listenTouchDirection();\n\nexport default $scroll;\nexport { Scroll };\n"],"mappings":";;;;;AAAA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA,IAAMA,MAAM,GAAG,EAAE;AAEjB,IAAMC,OAAO,GAAG,SAAVA,OAAOA,CAAaC,GAAG,EAAEC,EAAE,EAAE;EACjCH,MAAM,CAACI,IAAI,CAAC;IAAEF,GAAG,EAAHA,GAAG;IAAEC,EAAE,EAAFA;EAAG,CAAC,CAAC;EACxBA,EAAE,CAACE,MAAM,GAAGL,MAAM,CAACM,MAAM,GAAG,CAAC;AAC/B,CAAC;AAEDL,OAAO,CAACM,MAAM,GAAG,UAAUJ,EAAE,EAAE;EAC7BA,EAAE,CAACE,MAAM,IAAIL,MAAM,CAACQ,MAAM,CAACL,EAAE,CAACE,MAAM,EAAE,CAAC,CAAC;AAC1C,CAAC;;AAED;AACA,IAAMI,MAAM,GAAG;EACbC,UAAU,EAAE,SAAZA,UAAUA,CAAYC,OAAO,EAAEC,IAAI,EAAEC,OAAO,EAAE;IAC5C,IAAIF,OAAO,CAACG,gBAAgB,EAAEH,OAAO,CAACG,gBAAgB,CAACF,IAAI,EAAEC,OAAO,EAAE,KAAK,CAAC,CAAC,KACxE,IAAIF,OAAO,CAACI,WAAW,EAAEJ,OAAO,CAACI,WAAW,CAAC,IAAI,GAAGH,IAAI,EAAEC,OAAO,CAAC,CAAC,KACnEF,OAAO,CAAC,IAAI,GAAGC,IAAI,CAAC,GAAGC,OAAO;EACrC,CAAC;EACDG,oBAAoB,EAAE,SAAtBA,oBAAoBA,CAAA,EAAc;IAChC,IAAI,CAACN,UAAU,CAACO,MAAM,EAAE,QAAQ,EAAE,YAAY;MAC5C,IAAMC,EAAE,GAAGD,MAAM,CAACE,WAAW;QAC3BC,EAAE,GAAGH,MAAM,CAACI,OAAO;MACrBrB,MAAM,CACHsB,MAAM,CAAC,UAACC,CAAC;QAAA,OAAKA,CAAC,CAACrB,GAAG,CAACsB,YAAY,IAAID,CAAC,CAACrB,GAAG,CAACsB,YAAY,GAAG,CAAC;MAAA,EAAC,CAC3DC,OAAO,CAAC,UAACF,CAAC,EAAK;