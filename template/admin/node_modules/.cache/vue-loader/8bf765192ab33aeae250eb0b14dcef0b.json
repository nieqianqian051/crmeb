     <template slot-scope=\"scope\">\n            <span>{{ scope.row.filename }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"校验码\" min-width=\"130\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.cthash }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"上次访问时间\" min-width=\"130\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.atime }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"上次修改时间\" min-width=\"130\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.mtime }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"上次改变时间\" min-width=\"130\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.ctime }}</span>\n          </template>\n        </el-table-column>\n      </el-table>\n    </el-card>\n  </div>\n</template>\n\n<script>\nimport { fileListApi } from '@/api/system';\nexport default {\n  name: 'systemFile',\n  data() {\n    return {\n      loading: false,\n      tabList: [],\n    };\n  },\n  created() {\n    this.getList();\n  },\n  methods: {\n    // 列表\n    getList() {\n      this.loading = true;\n      fileListApi()\n        .then(async (res) => {\n          let data = res.data;\n          this.tabList = data.list;\n          this.loading = false;\n        })\n        .catch((res) => {\n          this.loading = false;\n          this.$message.error(res.msg);\n        });\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"]}]}